{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "consul": {
    "client": {
      "host": "localhost",
      "port": 8500,
      "scheme": "http",
      "datacenter" :  "yarp_dc"
    },
    "registration": {
      "name": "itemsapi",
      "tags": [ "webapi", "items", "webapi", "dotnet", "yarp" ],
      "meta" : {
        "yarp" : "on",
        "yarp_hosts" : "items",
        "yarp_path" : "/api/items/{**catchall}"
      },
      "port": 5010
    }
  }
}
